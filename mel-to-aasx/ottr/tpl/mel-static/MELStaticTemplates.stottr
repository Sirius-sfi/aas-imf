tpl:MelColumn [
	! ottr:IRI ?tag_number_entity,
	! ottr:IRI ?column_property,
	xsd:string ?value
] :: {
	ottr:Triple(?tag_number_entity, ?column_property, ?value)
} .

tpl:MelColumnDef [
	! ottr:IRI ?column_property,
	xsd:integer ?column_index
] :: {
	o-rdf:Type(mel:hasMelColumnValue, owl:DatatypeProperty),
	o-rdf:Type(?column_property, owl:DatatypeProperty),
	ottr:Triple(?column_property, rdfs:subPropertyOf, mel:hasMelColumnValue),
	ottr:Triple(?column_property, mel:hasColumnIndex, ?column_index),
	o-rdf:Type(mel:hasColumnIndex, owl:AnnotationProperty)
} .

tpl:TagNumberEntity [
	! ottr:IRI		?tag_number_entity,
	xsd:string		?tag_number,
	!? ottr:IRI		?tag_status,
	!? ottr:IRI		?discipline,
	? xsd:string	?cost_code_cor,
	!? ottr:IRI		?skid_ref,
	? xsd:string	?po_package_number,
	!? ottr:IRI		?system_number,
	? xsd:string	?equipment_type_description,
	!? ottr:IRI		?area,
	!? ottr:IRI		?sub_area,
	!? ottr:IRI		?function_area
] :: {
	o-rdf:Type(?tag_number_entity, mel:TagNumber),
	tpl:TagNumber(?tag_number_entity, ?tag_number),
	tpl:TagStatus(?tag_number_entity, ?tag_status),
	tpl:TagDiscipline(?tag_number_entity, ?discipline),
	tpl:TagCostCode(?tag_number_entity, ?cost_code_cor),
	tpl:TagSkidRef(?tag_number_entity, ?skid_ref),
	tpl:TagPOPackageNumber(?tag_number_entity, ?po_package_number),
	tpl:TagSystemNumber(?tag_number_entity, ?system_number),
	tpl:TagEquipmentTypeDescription(?tag_number_entity, ?equipment_type_description),
	tpl:TagArea(?tag_number_entity, ?area),
	tpl:TagSubArea(?tag_number_entity, ?sub_area),
	tpl:TagFunctionArea(?tag_number_entity, ?function_area)
} .

tpl:Discipline [
	! ottr:IRI		?discipline_entity,
	xsd:string		?rdfs_label,
	xsd:string		?discipline_code
] :: {
	o-rdf:Type(?discipline_entity, mel:Discipline),
	o-rdfs:Label(?discipline_entity, ?rdfs_label),
	ottr:Triple(?discipline_entity, mel:hasDisciplineCode, ?discipline_code)
} .

tpl:Area [
	! ottr:IRI		?area_entity,
	xsd:string		?rdfs_label,
	xsd:string		?area_code
] :: {
	o-rdf:Type(?area_entity, mel:Area),
	o-rdfs:Label(?area_entity, ?rdfs_label),
	ottr:Triple(?area_entity, mel:hasAreaCode, ?area_code)
} .

tpl:SubArea [
	! ottr:IRI		?sub_area_entity,
	xsd:string		?rdfs_label,
	xsd:string		?sub_area_code
] :: {
	o-rdf:Type(?sub_area_entity, mel:SubArea),
	o-rdfs:Label(?sub_area_entity, ?rdfs_label),
	ottr:Triple(?sub_area_entity, mel:hasSubAreaCode, ?sub_area_code)
} .

tpl:FunctionArea [
	! ottr:IRI		?function_area_entity,
	xsd:string		?rdfs_label,
	xsd:string		?function_area_code
] :: {
	o-rdf:Type(?function_area_entity, mel:FunctionArea),
	o-rdfs:Label(?function_area_entity, ?rdfs_label),
	ottr:Triple(?function_area_entity, mel:hasFunctionAreaCode, ?function_area_code)
} .

tpl:HasPart [
	! ottr:IRI		?whole_entity,
	! ottr:IRI		?part_entity
] :: {
	ottr:Triple(?whole_entity, mel:hasPart, ?part_entity)
} .

tpl:TopTag [ ! ottr:IRI ?tag_number_entity, xsd:boolean ?is_top_tag ] :: {
	ottr:Triple(?tag_number_entity, mel:isTopTag, ?is_top_tag)
} .

tpl:TagNumber[ ! ottr:IRI ?tag_number_entity, xsd:string ?tag_number ] :: {
	ottr:Triple(?tag_number_entity, mel:hasTagNumber, ?tag_number)
} .

tpl:TagStatus[ ! ottr:IRI ?tag_number_entity, ! ottr:IRI ?tag_status ] :: {
	ottr:Triple(?tag_number_entity, mel:hasTagStatus, ?tag_status)
} .

tpl:TagDiscipline[ ! ottr:IRI ?tag_number_entity, ! ottr:IRI ?discipline ] :: {
	ottr:Triple(?tag_number_entity, mel:hasDiscipline, ?discipline)
} .

tpl:TagCostCode[ ! ottr:IRI ?tag_number_entity, xsd:string ?cost_code_cor ] :: {
	ottr:Triple(?tag_number_entity, mel:hasCostCodeCOR, ?cost_code_cor)
} .

tpl:TagSkidRef[ ! ottr:IRI ?tag_number_entity, ! ottr:IRI ?skid_ref ] :: {
	ottr:Triple(?tag_number_entity, mel:hasSkidRef, ?skid_ref)
} .

tpl:TagPOPackageNumber[ ! ottr:IRI ?tag_number_entity, xsd:string ?po_package_number ] :: {
	ottr:Triple(?tag_number_entity, mel:hasPOPackageNumber, ?po_package_number)
} .

tpl:TagSystemNumber[ ! ottr:IRI ?tag_number_entity, ! ottr:IRI ?system_number ] :: {
	ottr:Triple(?tag_number_entity, mel:isPartOf, ?system_number)
} .

tpl:TagEquipmentTypeDescription[ ! ottr:IRI ?tag_number_entity, xsd:string ?equipment_type_description ] :: {
	ottr:Triple(?tag_number_entity, mel:hasEquipmentTypeDescription, ?equipment_type_description)
