tpl:TagArea[ ! ottr:IRI ?tag_number_entity, ! ottr:IRI ?area ] :: {
	ottr:Triple(?tag_number_entity, mel:isLocatedIn, ?area)
} .

tpl:TagSubArea[ ! ottr:IRI ?tag_number_entity, ! ottr:IRI ?sub_area ] :: {
	ottr:Triple(?tag_number_entity, mel:isLocatedIn, ?sub_area)
} .

tpl:TagFunctionArea[ ! ottr:IRI ?tag_number_entity, ! ottr:IRI ?function_area ] :: {
	ottr:Triple(?tag_number_entity, mel:isLocatedIn, ?function_area)
} .
